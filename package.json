{
  "name": "translectra",
  "version": "0.0.0",
  "description": "Open-source Translation Management System.",
  "license": "MIT",
  "main": "index.js",
  "engines": {
    "node": "9.11.1",
    "npm": "5.8.0"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "prestart": "npm run build",
    "start": "node build/index.js",
    "start:dev": "npm run watch",
    "test": "mocha 'src/**/*.spec.ts'",
    "watch": "concurrently \"npm run watch:build\" \"npm run watch:start\"",
    "watch:build": "webpack --config webpack.dev.js --watch",
    "watch:start": "nodemon --config nodemon.json build/index.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "bootstrap": "^4.1.0",
    "clean-webpack-plugin": "^0.1.19",
    "connect-flash": "^0.1.1",
    "dotenv": "^5.0.1",
    "jquery": "^3.3.1",
    "lexpress": "^0.39.2",
    "lodash": "^4.17.10",
    "moment": "^2.22.1",
    "mongoose": "^5.0.16",
    "mongoose-unique-validator": "^2.0.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local-mongoose": "^5.0.0",
    "popper.js": "^1.14.3",
    "ramda": "^0.25.0",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "style-loader": "^0.21.0",
    "webpack": "^4.6.0",
    "webpack-cli": "^2.0.15",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "mocha": "^5.1.1",
    "nodemon": "^1.17.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Electra-project/translectra.git"
  },
  "keywords": [],
  "author": {
    "name": "Electra Project",
    "email": "dev@electraproject.org",
    "url": "https://electraproject.org"
  },
